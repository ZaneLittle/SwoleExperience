{
  "name": "swole-experience-react",
  "version": "0.1.0",
  "private": true,
  "description": "SwoleExperience React Native (Expo) app with web support",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "~54.0.12",
    "expo-router": "~6.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.21.0",
    "victory": "^37.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@types/jest": "^29.5.12",
    "@testing-library/react-native": "^12.7.2",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-native-async-storage|react-native-uuid|victory|@testing-library|@babel|expo|expo-router|expo-status-bar|react-native-gesture-handler|react-native-safe-area-context|react-native-screens|react-native-svg|react-native-vector-icons|react-native-web)/)"
    ],
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testMatch": ["**/__tests__/**/*.(test|spec).(js|jsx|ts|tsx)"],
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,ts,tsx}",
      "hooks/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testEnvironment": "jsdom"
  }
}
